{
  "name": "alita-docs",
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "www"
      }
    },
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes":[{
    "src":"/(.*)",
    "dest":"server.js"
  }]
}
